# -*- coding: utf-8 -*-
#!/usr/bin/env python3
"""
Created on Fri Apr 27 09:50:07 2018

@author: tg23.park
"""

import csv
import sys
import random

input_file=sys.argv[1]

 

with open(input_file,'r',encoding='UTF8') as csv_in_file:
    filereader=csv.reader(csv_in_file)
    data=[]
    for row_list in filereader:
        value=float(row_list[0])
        data.append(value)

sample_mean=[]
for i in range(1000):       
    b=random.sample(data,k=100)
    mean=sum(b)/len(b)
    sample_mean.append(mean)

print(sum(sample_mean)/len(sample_mean)) 
